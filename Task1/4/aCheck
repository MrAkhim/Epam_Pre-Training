import java.util.Scanner;

public class Check {
	public static void main(String[] args) {
		int N, x4, x3, x2, x1;
		boolean b = false;
		Scanner sc = new Scanner(System.in);
		  N = sc.nextInt();
		
    x4 = N % 10;
		x3 = N % 10;
		x2 = N % 10;
		x1 = N % 10;
		
    if (((x4 < x3) && (x3 < x2) && (x2 < x1)) || 
				((x4 > x3) && (x3 > x2) && (x2 > x1))) 
        {
			b = true;
		  }
	}
}
